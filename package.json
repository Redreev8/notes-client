{
	"name": "notes",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently --kill-others \"vite\" \"npx tailwindcss -i ./public/index.css -o ./public/output.css --watch\"",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"pret": "npx prettier . --write",
		"preview": "vite preview"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint",
			"pre-push": "npm run pret"
		}
	},
	"lint-staged": {
		"*.(ts|tsx)": [
			"npm run lint -- --fix",
			"npm run pret",
			"git add"
		]
	},
	"overrides": {
		"eslint": "^9.13.0"
	},
	"dependencies": {
		"classnames": "^2.5.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.27.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.2",
		"autoprefixer": "^10.4.20",
		"concurrently": "^9.0.1",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.37.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"globals": "^15.11.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.10.0",
		"vite": "^5.4.8",
		"vite-plugin-svg-sprite": "^0.5.2"
	}
}
